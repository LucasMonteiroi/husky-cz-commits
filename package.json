{
  "name": "husky-cz-commits",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/LucasMonteiroi/husky-cz-commits.git",
  "license": "MIT",
  "scripts": {
    "post:install": "git config alias.cz '!npm run commit'",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "lint-staged": "lint-staged â€” config .lintstagedrc",
    "commit": "cz",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-emoji"
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "commitizen": "^4.2.4",
    "commitlint": "^16.2.3",
    "commitlint-config-gitmoji": "^2.2.5",
    "cz-conventional-emoji": "^1.0.2",
    "cz-customizable": "^6.3.0",
    "cz-emoji": "^1.3.1",
    "emoji-cz": "^0.3.1",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8"
  },
  "dependencies": {
    "express": "^4.17.3",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.6.3"
  },
  "description": "template project to develop express apis",
  "author": "Lucas Monteiro Inacio <lmsupport@outlook.com>"
}
